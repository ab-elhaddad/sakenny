    "dev:build": "npx tsc && npm run zip:clean && npm run www:clean && npm run zip",
    "migrate": "npx db-migrate up",
    "start:test": "set ENV=test&& nodemon src/index.ts",
    "start:prod": "set ENV=prod&& nodemon src/index.ts"
  },
